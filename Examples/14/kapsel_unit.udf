\begin{unit}
PI=3.141592654

[degree]=PI/180[rad]
[cal]=4.1855[J]
[kcal]=10^3[cal]
[R]=8.31451[J/(K*mol)]
[kB]=1.3806503e-23[J/K]
[Av]=6.0221367e23[mol^-1]
[Angstrom]=1.0e-10[m]
[bar]=1.0e5[Pa]
[fs]=10^-15[s]

[T]={$Unit_Parameter.Temperature}[K]
[L]={$Unit_Parameter.Length}[nm]
[rho]={$Unit_Parameter.rho}[g/cm^3]

[epsilon]=[kB*T]
[mass]=[rho*L^3]
[tau]=[mass^0.5*L*epsilon^-0.5]
[P]=[epsilon/L^3]
[eta]=[mass/(L*tau^2)*tau]

[kcal/mol]=Av^-1[kcal]
[kJ/mol]=1000.0*(Av^-1)[J]

\end{unit}